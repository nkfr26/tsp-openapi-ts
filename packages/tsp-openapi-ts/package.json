{
  "name": "@workspace/tsp-openapi-ts",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm dlx nodemon --watch . --ext tsp --exec pnpm run tsp-openapi-ts",
    "tsp-openapi-ts": "tsp compile . && pnpm dlx openapi-typescript ./tsp-output/schema/openapi.json -o ./openapi-ts-output/schema.d.ts"
  },
  "dependencies": {
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "openapi-typescript": "^7.8.0",
    "typescript": "^5.8.3"
  }
}
